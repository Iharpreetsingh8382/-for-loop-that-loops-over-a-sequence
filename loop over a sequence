# String example
word = "Python"
print("Looping through string:")
for char in word:
    print(char)

# Dictionary example
my_dict = {'a': 1, 'b': 2, 'c': 3}
print("\nLooping through dictionary keys and values:")
for key, value in my_dict.items():
    print(f"Key: {key}, Value: {value}")

# Sum of elements in a list
numbers = [10, 20, 30, 40, 50]
print("\nSumming the elements in the list:")
sum_of_numbers = 0
for num in numbers:
    sum_of_numbers += num
print(f"Sum: {sum_of_numbers}")
